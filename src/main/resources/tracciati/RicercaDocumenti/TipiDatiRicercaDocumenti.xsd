	</xs:simpleType>
	<xs:simpleType name="stringTypeMax20">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>	
	<xs:simpleType name="stringTypeMax30">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
