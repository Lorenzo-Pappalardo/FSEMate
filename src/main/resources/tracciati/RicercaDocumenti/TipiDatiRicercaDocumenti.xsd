			<xs:element name="fonte" type="stringTypeMax256" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="elencoErroriType">
		<xs:sequence>
			<xs:element name="Errore" type="erroreType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cfAssistitoType">
